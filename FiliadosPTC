CREATE DATABASE bdPartidoPTC

USE bdPartidoPTC

SELECT * FROM filiadosPTC

SELECT COUNT (NUMERO_DA_INSCRICAO)FROM filiadosPTC
--1
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE NOME_DO_FILIADO LIKE 'Antonio%'

--2
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE NOME_DO_FILIADO LIKE '%Sebastiao'

--3
SELECT NOME_DO_FILIADO FROM filiadosPTC 
WHERE NOME_DO_FILIADO LIKE 'Os%'

--4
SELECT NOME_DO_FILIADO, NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO LIKE 'SÃO FÉLIX'

--5
SELECT NOME_DO_FILIADO, NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO LIKE 'INHAMBUPE'

--6
SELECT NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO LIKE 'SANTA%'

--7
SELECT NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO LIKE 'E%'

--8
SELECT NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO LIKE '%CRUZ%'

--9
SELECT NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO LIKE '%DE%'

--10
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE DATA_DA_FILIACAO BETWEEN '01/01/1990' AND '01/01/2000'

--11
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE DATA_DA_FILIACAO BETWEEN '02/01/2010' AND '01/03/2010'

--12
SELECT NOME_DO_PARTIDO, NOME_DO_FILIADO FROM filiadosPTC
WHERE DATA_DA_FILIACAO BETWEEN '01/07/2007' AND '31/12/2007'

--13
SELECT NOME_DO_FILIADO,SITUACAO_DO_REGISTRO FROM filiadosPTC
WHERE SITUACAO_DO_REGISTRO LIKE '%REGULAR'

--14
SELECT NOME_DO_FILIADO,ZONA_ELEITORAL FROM filiadosPTC
WHERE ZONA_ELEITORAL = 203

 --15
SELECT NOME_DO_FILIADO,ZONA_ELEITORAL FROM filiadosPTC
WHERE ZONA_ELEITORAL = 180

--16
SELECT NOME_DO_FILIADO,ZONA_ELEITORAL FROM filiadosPTC
WHERE ZONA_ELEITORAL = 199

--17
SELECT ZONA_ELEITORAL FROM filiadosPTC
ORDER BY ZONA_ELEITORAL ASC
--18
/*NÃO FAZER*/

--19
UPDATE filiadosPTC
SET SIGLA_DO_PARTIDO='PT' 
WHERE CODIGO_DO_MUNICIPIO = 36471;
SELECT SIGLA_DO_PARTIDO,CODIGO_DO_MUNICIPIO FROM filiadosPTC
WHERE CODIGO_DO_MUNICIPIO = 36471

--20
UPDATE filiadosPTC
SET SIGLA_DO_PARTIDO='PMDB' 
WHERE CODIGO_DO_MUNICIPIO = 38490;
SELECT SIGLA_DO_PARTIDO,CODIGO_DO_MUNICIPIO FROM filiadosPTC
WHERE CODIGO_DO_MUNICIPIO = 38490

--21
UPDATE filiadosPTC
SET SIGLA_DO_PARTIDO='PC' 
WHERE NOME_DO_MUNICIPIO = 'SANTANÓPOLIS';
SELECT SIGLA_DO_PARTIDO,NOME_DO_MUNICIPIO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO = 'SANTANÓPOLIS'

--22
UPDATE filiadosPTC
SET SIGLA_DO_PARTIDO='PV' , NOME_DO_PARTIDO='Partido Verde dos Filiados'
WHERE NOME_DO_MUNICIPIO = 'SÃO FÉLIX';
SELECT SIGLA_DO_PARTIDO,NOME_DO_MUNICIPIO, NOME_DO_PARTIDO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO = 'SÃO FÉLIX'

--23
UPDATE filiadosPTC
SET SIGLA_DO_PARTIDO='PSDB' , NOME_DO_PARTIDO=' Partido da Social Democracia Brasileira '
WHERE NOME_DO_MUNICIPIO = 'ENCRUZILHADA';
SELECT SIGLA_DO_PARTIDO,NOME_DO_MUNICIPIO, NOME_DO_PARTIDO FROM filiadosPTC
WHERE NOME_DO_MUNICIPIO = 'ENCRUZILHADA'

--24
UPDATE filiadosPTC
SET NOME_DO_FILIADO='JORGE LUIS MAGALHÃES DOS SANTOS'
WHERE NOME_DO_FILIADO = 'JORGE LUIS FERREIRA DOS SANTOS'
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE NOME_DO_FILIADO = 'JORGE LUIS MAGALHÃES DOS SANTOS'

--25
UPDATE filiadosPTC
SET NOME_DO_FILIADO='RENATA XAVIER RODRIGUES'
WHERE NOME_DO_FILIADO = 'BENTA XAVIER RODRIGUES'

--26
UPDATE filiadosPTC
SET NOME_DO_FILIADO='NOEL LEITE DA SILVA'
WHERE NOME_DO_FILIADO = 'NOE LEITE DA SILVA'

--27
UPDATE filiadosPTC
SET SITUACAO_DO_REGISTRO='REGULAR'
WHERE NOME_DO_FILIADO = 'NOE LEITE DA SILVA'
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE SITUACAO_DO_REGISTRO = ''

--28
UPDATE filiadosPTC
SET SITUACAO_DO_REGISTRO='REGULAR'
WHERE NOME_DO_FILIADO = 'ELIEL ALMEIDA SILVA'
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE SITUACAO_DO_REGISTRO = 'DESFILIADO'

--29
UPDATE filiadosPTC
SET SITUACAO_DO_REGISTRO='DESFILIADO'
WHERE NOME_DO_FILIADO = 'ALINE DE SOUZA'
SELECT NOME_DO_FILIADO FROM filiadosPTC
WHERE SITUACAO_DO_REGISTRO = 'REGULAR'

--30
ALTER TABLE filiadosPTC
ADD Sexo CHAR(1);
